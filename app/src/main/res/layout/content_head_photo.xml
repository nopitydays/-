<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_head_photo"
    tools:context="com.example.gaohengdong.xiaodao.HeadPhotoActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h1"
                android:background="@drawable/h1"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h2"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/h2"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h3"
                android:layout_gravity="right"
                android:background="@drawable/h3"
                android:layout_margin="@dimen/head_margin"/>
            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h4"
                android:background="@drawable/h4"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/head_margin"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">


            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h5"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/h5"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h6"
                android:layout_gravity="right"
                android:background="@drawable/h6"
                android:layout_margin="@dimen/head_margin"/>
            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h7"
                android:background="@drawable/h7"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h8"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/h8"
                android:layout_margin="@dimen/head_margin"/>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">


            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h9"
                android:layout_gravity="right"
                android:background="@drawable/h9"
                android:layout_margin="@dimen/head_margin"/>
            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h10"
                android:background="@drawable/h10"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h11"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/h11"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h12"
                android:layout_gravity="right"
                android:background="@drawable/h12"
                android:layout_margin="@dimen/head_margin"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h13"
                android:background="@drawable/h13"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h14"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/h14"
                android:layout_margin="@dimen/head_margin"/>

            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h15"
                android:layout_gravity="right"
                android:background="@drawable/h15"
                android:layout_margin="@dimen/head_margin"/>
            <ImageView

                android:layout_width="@dimen/head_size"
                android:layout_height="@dimen/head_size"
                android:id="@+id/h16"
                android:background="@drawable/h16"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/head_margin"/>
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>
